<div class="container-fluid">
    <div class="row">
        <div id="map-column" class="col-xs-8">
            <div id='map'></div>
            <input id="google-search" type="text" class="form-control input-block-level" placeholder="Search For An Address Or Cross Street">
        </div>
        <div id="project-view-sidebar" class="col-xs-4">
          <div class="container-fluid">
            <div class="card" id="project-details">
                <div class="card-block">
                    <h4 class="card-title text-center" id="Proj_Title">Project View</h4>
                    <p class="card-text text-center" id="Proj_Desc">Click on a project on the map to view it's details.</p>
                </div>
                <ul class="list-group list-group-flush" id="common-attributes">
                    <li class="list-group-item">UID: <p style="display:inline" id="UID"></p></li>
                    <li class="list-group-item">Lead Agency: <p style="display:inline" id="Lead_Ag"></p></li>
                    <li class="list-group-item">Funding Status: <p style="display:inline" id="Fund_St"></p></li>
                    <li class="list-group-item">Project Manager: <p style="display:inline" id="Proj_Man"></p></li>
                    <li class="list-group-item">Contact Name: <p style="display:inline" id="Contact_info_name"></p></li>
                    <li class="list-group-item">Contact Number: <p style="display:inline" id="Contact_info_phone"></p></li>
                    <li class="list-group-item">Contact Email: <p style="display:inline" id="Contact_info_email"></p></li>
                    <li class="list-group-item">More Info: <p style="display:inline" id="More_info"></p></li>
                    <li class="list-group-item">CD: <p style="display:inline" id="CD"></p></li>
                </ul>

                <ul class="list-group list-group-flush" id="funded-attributes">
                  <li class="list-group-item">Department Project ID: <p style="display:inline" id="Dept_Proj_ID"></p></li>
                  <li class="list-group-item">Total Budget: <p style="display:inline" id="Total_bgt"></p></li>
                  <li class="list-group-item">Grant: <p style="display:inline" id="Grant"></p></li>
                  <li class="list-group-item">Other Funds: <p style="display:inline" id="Other_funds"></p></li>
                  <li class="list-group-item">Prop C: <p style="display:inline" id="Prop_c"></p></li>
                  <li class="list-group-item">Measure R: <p style="display:inline" id="Measure_r"></p></li>
                  <li class="list-group-item">General Fund: <p style="display:inline" id="General_fund"></p></li>
                  <li class="list-group-item">Current Status: <p style="display:inline" id="Current_Status"></p></li>
                  <li class="list-group-item">Issues: <p style="display:inline" id="Issues"></p></li>
                  <li class="list-group-item">At Risk of Deobligation: <p style="display:inline" id="Deobligation"></p></li>
                  <li class="list-group-item">Explanation: <p style="display:inline" id="Explanation"></p></li>
                  <li class="list-group-item">Other Project ID: <p style="display:inline" id="Other_ID"></p></li>
                  <li class="list-group-item">Construction By: <p style="display:inline" id="Constr_by"></p></li>
                  <li class="list-group-item">Info Source: <p style="display:inline" id="Info_source"></p></li>
                </ul>

                <ul class="list-group list-group-flush" id="unfunded-attributes">
                  <li class="list-group-item">Grant Category: <p style="display:inline" id="Grant_Cat"></p></li>
                  <li class="list-group-item">Project Type: <p style="display:inline" id="Proj_Ty"></p></li>
                  <li class="list-group-item">Estimated Cost: <p style="display:inline" id="Est_Cost"></p></li>
                  <li class="list-group-item">Fund Request: <p style="display:inline" id="Fund_Rq"></p></li>
                  <li class="list-group-item">Local Match: <p style="display:inline" id="Lc_match"></p></li>
                  <li class="list-group-item">Match Percentage: <p style="display:inline" id="Match_Pt"></p></li>
                  <li class="list-group-item">Comments: <p style="display:inline" id="Comments"></p></li>
                </ul>
                <!-- <div class="card-block">
                    <a href="#" class="card-link">Card link</a>
                    <a href="#" class="card-link">Another link</a>
                </div> -->
            </div>
          </div>
        </div>
    </div>

    <script type="text/javascript" src='/javascripts/map.js'></script>
